                                                    <span data-i18n="firmname"></span>
                                                    <span class="text-muted text-lowercase fs-12px"
                                                          data-i18n="anlihouse-A19"></span>
                                                </label>

                                                <span class="input-group">
                                            {{ shipping_form.shipping_company_name(type='text', id='shippingAddressCompanyName', class='form-control') }}
                                        </span>
                                                <small class="text-danger input-error shipping-address-company-name-error d-none">
                                                    <span data-i18n=""></span>
                                                </small>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                                {# End Company Name Tax #}

                                {# Start Country #}
                                <div class="row">
                                    <div class="col-12">
                                        <div class="mb-3">
                                            <div class="form-group">
                                                <label for="shippingAddressCompanyName" class="form-label w-100">
                                                    <span data-i18n="country"></span>
                                                    <span class="text-danger text-lowercase fs-12px"
                                                          data-i18n="anlihouse-A18"></span>
