                                    <label class="form-label w-100">
                                        <span data-i18n="i18n_registration_07"></span>
                                        <span class="text-danger text-lowercase fs-12px"
                                              data-i18n="anlihouse-A18"></span>
                                    </label>

                                    <div style="line-height:20px;" class="radio-input float-left">
                                        <input class="billingAddressIsShippingAddressYes" type="radio"
                                               id="billingAddressIsShippingAddressYes"
                                               name="billingAddressIsShippingAddress"
                                               {% if is_user_login['user_billing_information']['is_shipping_address'] == "True" %}checked="checked"{% endif %}>
                                        <label class="d-content" for="billingAddressIsShippingAddressYes"
                                               data-i18n="igen"></label>
                                    </div>

                                    <div style="line-height:20px;" class="radio-input float-left ml-3">
                                        <input class="billingAddressIsShippingAddressNo" type="radio"
                                               id="billingAddressIsShippingAddressNo"
                                               name="billingAddressIsShippingAddress"
                                               {% if is_user_login['user_billing_information']['is_shipping_address'] == "False" %}checked="checked"{% endif %}>
                                        <label class="d-content" for="billingAddressIsShippingAddressNo"
                                               data-i18n="nem"></label>
                                    </div>

                                    <small class="text-danger input-error billing-address-is-shipping-address-error d-none w-100 d-inline-block">
                                        <span data-i18n=""></span>
                                    </small>
                                </div>
                                {# End Is Shipping Address #}

                            </div>
                            {# End Billing Address Form Input #}

                            {# Start Shipping Address Form Input #}
