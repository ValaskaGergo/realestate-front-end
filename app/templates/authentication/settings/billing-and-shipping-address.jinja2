                                            <small class="text-danger input-error shipping-address-first-name-error d-none">
                                                <span data-i18n=""></span>
                                            </small>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6 col-xl-6 col-xxl-6">
                                        <div class="mb-3">
                                            <label for="shippingAddressLastName" class="form-label">
                                                <span data-i18n="lastname"></span>
                                                <span class="text-danger text-lowercase fs-12px"
                                                      data-i18n="anlihouse-A18"></span>
                                            </label>
                                            <span class="input-group">
                                            {{ shipping_form.shipping_last_name(type='text', id='shippingAddressLastName', class='form-control') }}
                                        </span>
                                            <small class="text-danger input-error shipping-address-last-name-error d-none">
                                                <span data-i18n=""></span>
                                            </small>
                                        </div>
                                    </div>

                                </div>
                                {# End First Last Name #}

                                {# Start Phone #}
                                <div class="row">
                                    <div class="col-12">
                                        <div class="mb-3">
                                            <label for="shippingAddressPhone" class="form-label">
                                                <span data-i18n="phone"></span>
                                                <span class="text-danger text-lowercase fs-12px"
                                                      data-i18n="anlihouse-A18"></span>
                                            </label>
                                            <div class="input-group">
                                                <span class="input-group-text shipping-phone-country-code d-none"></span>
                                                {{ shipping_form.shipping_phone(type='text', id='shippingAddressPhone', class='form-control', style='border-radius:0.375rem !important;') }}
                                            </div>
                                            <small class="text-danger input-error shipping-address-phone-error d-none">
                                                <span data-i18n=""></span>
                                            </small>
                                        </div>
                                    </div>
                                </div>
                                {# End Phone #}

                                {# Start Email #}
                                <div class="row">
                                    <div class="col-12">
                                        <div class="mb-3">
                                            <label for="shippingAddressEmail" class="form-label">
                                                <span data-i18n="email"></span>
                                                <span class="text-danger text-lowercase fs-12px"
                                                      data-i18n="anlihouse-A18"></span>
                                            </label>
                                            <div class="input-group">
                                                <span class="input-group-text shipping-email-country-code d-none"></span>
