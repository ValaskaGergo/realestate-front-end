{% extends "layout.jinja2" %}

{# Start Breadcrumb #}
{% block breadcrumb %}
    <div id="breadcrumb">

        <div class="container w-100 mw-100 pl-3 pr-3">
            <div class="row">
                <div class="col-12">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a class="hover" href="{{ url_for('index_module.index') }}"
                                                           data-i18n="anlihouse-anlihouse"></a>
                            </li>
                            <li class="breadcrumb-item"><a class="hover" href="{{ url_for('index_module.index') }}"
                                                           data-i18n="anlihouse-anlibreeders"></a></li>
                            <li class="breadcrumb-item active" aria-current="page" data-i18n="anlihouse-A65"></li>
                        </ol>
                    </nav>
                </div>
            </div>
        </div>

    </div>
{% endblock %}
{# End Breadcrumb #}

{# Start Content #}
{% block content %}
    <div class="container billingAndShippingAddress">
        <div class="row">
            <div class="col-12">

                <div class="card">
                    <div class="card-header" data-i18n="anlihouse-A65"></div>
                    <div class="card-body">

                        <form id="billing-address-form" onsubmit="return false" novalidate="novalidate">
                            {{ billing_form.csrf_token }}

                            {{ billing_form.user_email(type='hidden', id='billingAddressUserEmail', class='form-control') }}
                            {{ billing_form.billing_is_company(type='hidden', id='billingAddressIsCompany', class='form-control') }}
                            {{ billing_form.billing_is_shipping_address(type='hidden', id='billingAddressIsShipping', class='form-control') }}
                            {{ billing_form.billing_country(type='hidden', id='billingAddressCountry', class='form-control') }}
                            {{ billing_form.billing_currency(type='hidden', id='billingAddressCurrency', class='form-control') }}

                            {{ shipping_form.shipping_country(type='hidden', id='shippingAddressCountry', class='form-control') }}
                            {{ shipping_form.shipping_currency(type='hidden', id='shippingAddressCurrency', class='form-control') }}

                            <div class="alert alert-danger form-alert-danger mb-3 d-none" role="alert"
                                 data-i18n="anlihouse-A21">
                            </div>

                            {# Start Billing Address Form Input #}
                            <div id="billingAddress">

                                {# Start Is Company Radio #}
                                <div class="mb-3 d-inline-block">
                                    <label class="form-label w-100">
                                        <span data-i18n="i18n_registration_02"></span>
                                        <span class="text-danger text-lowercase fs-12px"
                                              data-i18n="anlihouse-A18"></span>
                                    </label>

                                    <div class="radio-input float-left">
                                        <input class="billingAddressIsCompanyPerson" type="radio"
                                               id="billingAddressIsCompanyPerson"
                                               name="billingAddressIsCompany"
                                               {% if is_user_login['user_billing_information']['is_company'] == "False" %}checked="checked"{% endif %}>
                                        <label class="d-content" for="billingAddressIsCompanyPerson"
                                               data-i18n="i18n_registration_04"></label>
                                    </div>

                                    <div class="radio-input float-left ml-3">
                                        <input class="billingAddressIsCompanyCompany" type="radio"
                                               id="billingAddressIsCompanyCompany"
                                               name="billingAddressIsCompany"
                                               {% if is_user_login['user_billing_information']['is_company'] == "True" %}checked="checked"{% endif %}>
                                        <label class="d-content" for="billingAddressIsCompanyCompany"
                                               data-i18n="i18n_registration_03"></label>
                                    </div>

                                    <small class="text-danger input-error billing-address-is-company-error d-none w-100 d-inline-block">
                                        <span data-i18n=""></span>
                                    </small>
                                </div>
                                {# End Is Company Radio #}

                                <h5 class="mb-3 mt-3" data-i18n="billingaddress"></h5>

                                {# Start First Last Name #}
                                <div class="row">
                                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6 col-xl-6 col-xxl-6">
                                        <div class="mb-3">
                                            <label for="billingAddressFirstName" class="form-label">
                                                <span data-i18n="firstname"></span>
                                                <span class="text-danger text-lowercase fs-12px"
                                                      data-i18n="anlihouse-A18"></span>
                                            </label>

                                            <span class="input-group">
                                            {{ billing_form.billing_first_name(type='text', id='billingAddressFirstName', class='form-control') }}
                                        </span>
                                            <small class="text-danger input-error billing-address-first-name-error d-none">
                                                <span data-i18n=""></span>
                                            </small>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6 col-xl-6 col-xxl-6">
                                        <div class="mb-3">
                                            <label for="billingAddressLastName" class="form-label">
                                                <span data-i18n="lastname"></span>
                                                <span class="text-danger text-lowercase fs-12px"
                                                      data-i18n="anlihouse-A18"></span>
                                            </label>
                                            <span class="input-group">
                                            {{ billing_form.billing_last_name(type='text', id='billingAddressLastName', class='form-control') }}
                                        </span>
                                            <small class="text-danger input-error billing-address-last-name-error d-none">
                                                <span data-i18n=""></span>
                                            </small>
                                        </div>
                                    </div>

                                </div>
                                {# End First Last Name #}

                                {# Start Phone #}
                                <div class="row">
                                    <div class="col-12">
                                        <div class="mb-3">
                                            <label for="billingAddressPhone" class="form-label">
                                                <span data-i18n="phone"></span>
                                                <span class="text-danger text-lowercase fs-12px"
                                                      data-i18n="anlihouse-A18"></span>
                                            </label>
                                            <div class="input-group">
                                                <span class="input-group-text billing-phone-country-code d-none"></span>
                                                {{ billing_form.billing_phone(type='text', id='billingAddressPhone', class='form-control') }}
                                            </div>
                                            <small class="text-danger input-error billing-address-phone-error d-none">
                                                <span data-i18n=""></span>
                                            </small>
                                        </div>
                                    </div>
                                </div>
                                {# End Phone #}

                                {# Start Email #}
                                <div class="row">
                                    <div class="col-12">
                                        <div class="mb-3">
                                            <label for="billingAddressEmail" class="form-label">
                                                <span data-i18n="email"></span>
                                                <span class="text-danger text-lowercase fs-12px"
                                                      data-i18n="anlihouse-A18"></span>
                                            </label>
                                            <div class="input-group">
                                                <span class="input-group-text billing-email-country-code d-none"></span>
                                                {{ billing_form.billing_email(type='email', id='billingAddressEmail', class='form-control', style='border-radius:0.375rem !important;') }}
                                            </div>
                                            <small class="text-danger input-error billing-address-email-error d-none">
                                                <span data-i18n=""></span>
                                            </small>
                                        </div>
                                    </div>
                                </div>
                                {# End Email #}

                                {# Start Company Name Tax #}
