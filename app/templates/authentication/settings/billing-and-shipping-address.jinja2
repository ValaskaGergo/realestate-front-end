                                                                data-tokens="{{ country['country_native'] }}"
                                                                data-id="{{ country['id'] }}"
                                                                data-country-code="{{ country['country_code'] }}"
                                                                data-currency="{{ country['currency_symbol'] }}">{{ country['country_code'] }}
                                                            - {{ country['country'] }}</option>
                                                    {% endfor %}
                                                    <option data-divider="true"></option>

                                                    <option disabled
                                                            style="color:#4c6180;">{{ countries['Europe'][0]['region'] }}</option>
                                                    {% for country in countries['Europe'] %}
                                                        <option value="{{ country['country_code'] }}"
                                                                data-tokens="{{ country['country_native'] }}"
                                                                data-id="{{ country['id'] }}"
                                                                data-country-code="{{ country['country_code'] }}"
                                                                data-currency="{{ country['currency_symbol'] }}">{{ country['country_code'] }}
                                                            - {{ country['country'] }}</option>
                                                    {% endfor %}
                                                    <option data-divider="true"></option>

                                                    <option disabled
                                                            style="color:#4c6180;">{{ countries['Asia'][0]['region'] }}</option>
                                                    {% for country in countries['Asia'] %}
                                                        <option value="{{ country['country_code'] }}"
                                                                data-tokens="{{ country['country_native'] }}"
                                                                data-id="{{ country['id'] }}"
                                                                data-country-code="{{ country['country_code'] }}"
                                                                data-currency="{{ country['currency_symbol'] }}">{{ country['country_code'] }}
                                                            - {{ country['country'] }}</option>
                                                    {% endfor %}
                                                    <option data-divider="true"></option>

                                                    <option disabled
                                                            style="color:#4c6180;">{{ countries['Africa'][0]['region'] }}</option>
                                                    {% for country in countries['Africa'] %}
                                                        <option value="{{ country['country_code'] }}"
                                                                data-tokens="{{ country['country_native'] }}"
                                                                data-id="{{ country['id'] }}"
                                                                data-country-code="{{ country['country_code'] }}"
                                                                data-currency="{{ country['currency_symbol'] }}">{{ country['country_code'] }}
                                                            - {{ country['country'] }}</option>
                                                    {% endfor %}
                                                    <option data-divider="true"></option>

                                                    <option disabled
                                                            style="color:#4c6180;">{{ countries['Oceania'][0]['region'] }}</option>
                                                    {% for country in countries['Oceania'] %}
                                                        <option value="{{ country['country_code'] }}"
                                                                data-tokens="{{ country['country_native'] }}"
                                                                data-id="{{ country['id'] }}"
                                                                data-country-code="{{ country['country_code'] }}"
                                                                data-currency="{{ country['currency_symbol'] }}">{{ country['country_code'] }}
                                                            - {{ country['country'] }}</option>
                                                    {% endfor %}
                                                    <option data-divider="true"></option>

                                                    <option disabled
                                                            style="color:#4c6180;">{{ countries['Antarctic'][0]['region'] }}</option>
                                                    {% for country in countries['Antarctic'] %}
                                                        <option value="{{ country['country_code'] }}"
                                                                data-tokens="{{ country['country_native'] }}"
                                                                data-id="{{ country['id'] }}"
                                                                data-country-code="{{ country['country_code'] }}"
                                                                data-currency="{{ country['currency_symbol'] }}">{{ country['country_code'] }}
                                                            - {{ country['country'] }}</option>
                                                    {% endfor %}

                                                </select>

                                                <small class="text-danger input-error billing-address-country-error d-none">
                                                    <span data-i18n=""></span>
                                                </small>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                                {# End Country #}


                                {# Start Zip Place #}
                                <div class="row">
                                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6 col-xl-6 col-xxl-6">
                                        <div class="mb-3">
                                            <label for="billingAddressZipNumber" class="form-label">
                                                <span data-i18n="zip"></span>
                                                <span class="text-danger text-lowercase fs-12px"
                                                      data-i18n="anlihouse-A18"></span>
                                            </label>

                                            <span class="input-group">
                                            {{ billing_form.billing_zip_number(type='text', id='billingAddressZipNumber', class='form-control') }}
                                        </span>
                                            <small class="text-danger input-error billing-address-zip-number-error d-none">
                                                <span data-i18n=""></span>
                                            </small>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6 col-xl-6 col-xxl-6">
                                        <div class="mb-3">
                                            <label for="billingAddressCompanyPlace" class="form-label">
                                                <span data-i18n="i18n_registration_06"></span>
                                                <span class="text-danger text-lowercase fs-12px"
                                                      data-i18n="anlihouse-A18"></span>
                                            </label>
                                            <span class="input-group">
                                            {{ billing_form.billing_place(type='text', id='billingAddressPlace', class='form-control') }}
                                        </span>
                                            <small class="text-danger input-error billing-address-place-error d-none">
                                                <span data-i18n=""></span>
                                            </small>
                                        </div>
                                    </div>

                                </div>
                                {# End Zip Place #}

                                {# Start Street #}
                                <div class="row">
                                    <div class="col-12">
                                        <div class="mb-3">
                                            <label for="billingAddressStreet" class="form-label">
                                                <span data-i18n="anlihouse-A74"></span>
                                                <span class="text-danger text-lowercase fs-12px"
                                                      data-i18n="anlihouse-A18"></span>
                                            </label>
                                            {{ billing_form.billing_street(type='text', id='billingAddressStreet', class='form-control') }}
                                            <small class="text-danger input-error billing-address-street-error d-none">
                                                <span data-i18n=""></span>
                                            </small>
                                        </div>
                                    </div>
                                </div>
                                {# End Street #}

                                {# Start Is Shipping Address #}
