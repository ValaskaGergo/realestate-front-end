                                                                data-country-code="{{ country['country_code'] }}"
                                                                data-currency="{{ country['currency_symbol'] }}">{{ country['country_code'] }}
                                                            - {{ country['country'] }}</option>
                                                    {% endfor %}
                                                    <option data-divider="true"></option>

                                                    <option disabled
