                                                <select class="shippingAddressCountrySelect form-control"
                                                        title="Ex.: US - UNITED STATES" disabled>
                                                    <option disabled
                                                            style="color:#4c6180;">{{ countries['Americas'][0]['region'] }}</option>
                                                    {% for country in countries['Americas'] %}
