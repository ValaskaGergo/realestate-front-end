{% extends "layout.jinja2" %}

{% block title %}{% endblock %}

{# Start Breadcrumb #}
{% block breadcrumb %}
    <div id="breadcrumb" class="snapping">

        <div class="container w-100 mw-100 pl-3 pr-3">
            <div class="row">
                <div class="col-12">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a class="hover" href="{{ url_for('index_module.index') }}"
                                                           data-i18n="anlihouse-anlihouse"></a></li>
                            {% if (only_one_category_id == None or only_one_category_id == "None") and (only_one_subcategory_id == None or only_one_subcategory_id == "None") and (seller_user_name == None or seller_user_name == "None") and (wishlist == None or wishlist == "None") %}
                                <li class="breadcrumb-item active" aria-current="page"
                                    data-i18n="anlihouse-anlibreeders"></li>
                            {% elif (only_one_category_id != None or only_one_category_id != "None") and (only_one_subcategory_id == None or only_one_subcategory_id == "None") and (seller_user_name == None or seller_user_name == "None") and (wishlist == None or wishlist == "None") %}
                                <li class="breadcrumb-item"><a class="hover" href="{{ url_for('index_module.index') }}"
                                                               data-i18n="anlihouse-anlibreeders"></a></li>
                                <li class="breadcrumb-item active" aria-current="page">{{ only_one_category_name }}</li>
                            {% elif (only_one_category_id != None or only_one_category_id != "None") and (only_one_subcategory_id != None or only_one_subcategory_id != "None") and (seller_user_name == None or seller_user_name == "None") and (wishlist == None or wishlist == "None") %}
                                <li class="breadcrumb-item"><a class="hover" href="{{ url_for('index_module.index') }}"
                                                               data-i18n="anlihouse-anlibreeders"></a></li>
                                <li class="breadcrumb-item"><a class="hover"
                                                               href="{{ url_for('index_module.category_list', category_name=only_one_category_name_slug, category_id=only_one_category_id) }}">{{ only_one_category_name }}</a>
                                </li>
                                <li class="breadcrumb-item active"
                                    aria-current="page">{{ only_one_subcategory_name }}</li>
                            {% elif (only_one_category_id == None or only_one_category_id == "None") and (only_one_subcategory_id == None or only_one_subcategory_id == "None") and (seller_user_name != None or seller_user_name != "None") and (wishlist == None or wishlist == "None") %}
                                <li class="breadcrumb-item"><a class="hover" href="{{ url_for('index_module.index') }}"
                                                               data-i18n="anlihouse-anlibreeders"></a></li>
                                <li class="breadcrumb-item active seller_user_name" aria-current="page"
                                    data-seller_user_name="{{ seller_user_name }}">{{ seller_user_name }}</li>
                            {% elif (only_one_category_id != None or only_one_category_id != "None") and (only_one_subcategory_id == None or only_one_subcategory_id == "None") and (seller_user_name != None or seller_user_name != "None") and (wishlist == None or wishlist == "None") %}
                                <li class="breadcrumb-item"><a class="hover" href="{{ url_for('index_module.index') }}"
                                                               data-i18n="anlihouse-anlibreeders"></a></li>
                                <li class="breadcrumb-item seller_user_name"
                                    data-seller_user_name="{{ seller_user_name }}"><a class="hover"
                                                                                      href="{{ url_for('index_module.user_animal_list', user_id=seller_user_id, user_name=seller_user_name_slug) }}">{{ seller_user_name }}</a>
                                </li>
                                <li class="breadcrumb-item active" aria-current="page">{{ only_one_category_name }}</li>
                            {% elif (only_one_category_id != None or only_one_category_id != "None") and (only_one_subcategory_id != None or only_one_subcategory_id != "None") and (seller_user_name != None or seller_user_name != "None") and (wishlist == None or wishlist == "None") %}
                                <li class="breadcrumb-item"><a class="hover" href="{{ url_for('index_module.index') }}"
                                                               data-i18n="anlihouse-anlibreeders"></a></li>
                                <li class="breadcrumb-item seller_user_name"
                                    data-seller_user_name="{{ seller_user_name }}"><a class="hover"
                                                                                      href="{{ url_for('index_module.user_animal_list', user_id=seller_user_id, user_name=seller_user_name_slug) }}">{{ seller_user_name }}</a>
                                </li>
                                <li class="breadcrumb-item"><a class="hover"
                                                               href="{{ url_for('index_module.category_list', category_name=only_one_category_name_slug, category_id=only_one_category_id) }}">{{ only_one_category_name }}</a>
                                </li>
                                <li class="breadcrumb-item active"
                                    aria-current="page">{{ only_one_subcategory_name }}</li>
                            {% elif (only_one_category_id == None or only_one_category_id == "None") and (only_one_subcategory_id == None or only_one_subcategory_id == "None") and (seller_user_name == None or seller_user_name == "None") and (wishlist != None or wishlist != "None") %}
                                <li class="breadcrumb-item"><a class="hover" href="{{ url_for('index_module.index') }}"
                                                               data-i18n="anlihouse-anlibreeders"></a></li>
                                <li class="breadcrumb-item active wishlist" aria-current="page"
                                    data-i18n="anlihouse-A377"></li>
                            {% elif (only_one_category_id != None or only_one_category_id != "None") and (only_one_subcategory_id == None or only_one_subcategory_id == "None") and (seller_user_name == None or seller_user_name == "None") and (wishlist != None or wishlist != "None") %}
                                <li class="breadcrumb-item"><a class="hover" href="{{ url_for('index_module.index') }}"
                                                               data-i18n="anlihouse-anlibreeders"></a></li>
                                <li class="breadcrumb-item wishlist"><a class="hover"
                                                                        href="{{ url_for('index_module.user_animal_wishlist') }}"
                                                                        data-i18n="anlihouse-A377"></a></li>
                                <li class="breadcrumb-item active" aria-current="page">{{ only_one_category_name }}</li>
                            {% elif (only_one_category_id != None or only_one_category_id != "None") and (only_one_subcategory_id != None or only_one_subcategory_id != "None") and (seller_user_name == None or seller_user_name == "None") and (wishlist != None or wishlist != "None") %}
                                <li class="breadcrumb-item"><a class="hover" href="{{ url_for('index_module.index') }}"
                                                               data-i18n="anlihouse-anlibreeders"></a></li>
                                <li class="breadcrumb-item wishlist"><a class="hover"
                                                                        href="{{ url_for('index_module.user_animal_wishlist') }}"
                                                                        data-i18n="anlihouse-A377"></a></li>
                                <li class="breadcrumb-item"><a class="hover"
                                                               href="{{ url_for('index_module.category_list', category_name=only_one_category_name_slug, category_id=only_one_category_id) }}">{{ only_one_category_name }}</a>
                                </li>
                                <li class="breadcrumb-item active"
                                    aria-current="page">{{ only_one_subcategory_name }}</li>
                            {% else %}
                            {% endif %}
                        </ol>
                    </nav>
                </div>
            </div>
        </div>

    </div>
{% endblock %}
{# End Breadcrumb #}

{# Start Content #}
{% block content %}

    <div id="list" class="container">

        <div id="list-top">
            <div id="filter-info-wrap">
                <ul>
                    {% if animals == [] %}
                        <li class="elem active"><span data-i18n="anlihouse-A292"></span>:</li>
                        <li class="elem active" data-i18n="anlihouse-A379"></li>
                    {% else %}
                        <li class="elem active"><span data-i18n="anlihouse-A292"></span>:</li>
                        <li class="elem active filter-info-type" data-i18n="anlihouse-A293"></li>
                    {% endif %}
                </ul>
            </div>

            {# Start List View #}
            <div id="filter-dropdown-wrap" class="{% if animals == [] %}pointer-events-none opacity05{% endif %}">
                <div class="dropdown">
                    <span class="btn btn-secondary dropdown-toggle shadow" role="button" id="dropdownSnapView"
                          data-bs-toggle="dropdown" aria-expanded="false">
                        <svg xmlns="http://www.w3.org/2000/svg" width="28" height="28" fill="currentColor"
                             class="bi bi-list-ol" viewBox="0 0 16 16">
                            <path fill-rule="evenodd"
                                  d="M5 11.5a.5.5 0 0 1 .5-.5h9a.5.5 0 0 1 0 1h-9a.5.5 0 0 1-.5-.5zm0-4a.5.5 0 0 1 .5-.5h9a.5.5 0 0 1 0 1h-9a.5.5 0 0 1-.5-.5zm0-4a.5.5 0 0 1 .5-.5h9a.5.5 0 0 1 0 1h-9a.5.5 0 0 1-.5-.5z"/><path
                                d="M1.713 11.865v-.474H2c.217 0 .363-.137.363-.317 0-.185-.158-.31-.361-.31-.223 0-.367.152-.373.31h-.59c.016-.467.373-.787.986-.787.588-.002.954.291.957.703a.595.595 0 0 1-.492.594v.033a.615.615 0 0 1 .569.631c.003.533-.502.8-1.051.8-.656 0-1-.37-1.008-.794h.582c.008.178.186.306.422.309.254 0 .424-.145.422-.35-.002-.195-.155-.348-.414-.348h-.3zm-.004-4.699h-.604v-.035c0-.408.295-.844.958-.844.583 0 .96.326.96.756 0 .389-.257.617-.476.848l-.537.572v.03h1.054V9H1.143v-.395l.957-.99c.138-.142.293-.304.293-.508 0-.18-.147-.32-.342-.32a.33.33 0 0 0-.342.338v.041zM2.564 5h-.635V2.924h-.031l-.598.42v-.567l.629-.443h.635V5z"/>
                        </svg>
                        <span data-i18n="anlihouse-A380"></span>
                    </span>

                    <ul id="dropdownSnapViews" class="dropdown-menu shadow" aria-labelledby="dropdownSnapView">
                        <li><span class="dropdown-item filter-dropdown active_" data-type="list"
                                  data-i18n="anlihouse-A381"></span>
                        </li>
                        <li>
                            <hr class="dropdown-divider">
                        </li>
                        <li><span class="dropdown-item filter-dropdown inactive_" data-type="snap"
                                  data-i18n="anlihouse-A382"></span>
                        </li>
                    </ul>
                </div>
            </div>
            {# End List View #}

            {# Start Order #}
            <div id="filter-dropdown-wrap" class="{% if animals == [] %}pointer-events-none opacity05{% endif %}"
                 style="margin-right:15px;">
                <div class="dropdown">
                    <span class="btn btn-secondary dropdown-toggle shadow" role="button" id="dropdownFilterLink"
                          data-bs-toggle="dropdown" aria-expanded="false">
                        <svg xmlns="http://www.w3.org/2000/svg" width="28" height="28" fill="currentColor"
                             class="bi bi-filter" viewBox="0 0 16 16">
                            <path d="M6 10.5a.5.5 0 0 1 .5-.5h3a.5.5 0 0 1 0 1h-3a.5.5 0 0 1-.5-.5zm-2-3a.5.5 0 0 1 .5-.5h7a.5.5 0 0 1 0 1h-7a.5.5 0 0 1-.5-.5zm-2-3a.5.5 0 0 1 .5-.5h11a.5.5 0 0 1 0 1h-11a.5.5 0 0 1-.5-.5z"/>
                        </svg>
                        <span data-i18n="anlihouse-A292"></span>
                    </span>

                    <ul id="dropdownFilterLinks" class="dropdown-menu shadow" aria-labelledby="dropdownFilterLink">
                        <li><span class="dropdown-item filter-dropdown" data-type="price-asc"
                                  data-i18n="anlihouse-A293"></span>
                        </li>
                        <li><span class="dropdown-item filter-dropdown" data-type="price-desc"
                                  data-i18n="anlihouse-A294"></span>
                        </li>
                        <li><span class="dropdown-item filter-dropdown" data-type="age-asc"
                                  data-i18n="anlihouse-A295"></span>
                        </li>
                        <li><span class="dropdown-item filter-dropdown" data-type="age-desc"
                                  data-i18n="anlihouse-A296"></span>
                        </li>
                        <li><span class="dropdown-item filter-dropdown" data-type="height-asc"
                                  data-i18n="anlihouse-A297"></span>
                        </li>
                        <li><span class="dropdown-item filter-dropdown" data-type="height-desc"
                                  data-i18n="anlihouse-A298"></span></li>
                        <li><span class="dropdown-item filter-dropdown" data-type="rating-asc"
                                  data-i18n="anlihouse-A299"></span>
                        </li>
                        <li><span class="dropdown-item filter-dropdown" data-type="rating-desc"
                                  data-i18n="anlihouse-A300"></span></li>
                        <li>
                            <hr class="dropdown-divider">
                        </li>
                        <li><span class="dropdown-item filter-dropdown" data-type="search-jump"
                                  data-i18n="anlihouse-A291"></span></li>
                    </ul>
                </div>
            </div>
            {# End Order #}
        </div>

        <div id="snap-wrap">
            <div class="row infinite-container">

                {% for animal in animals %}
                    <div id="snap-{{ animal['animal']['id'] }}" class="box col-12 infinite-item snap-list"
                         data-section-name="{{ animal['animal']['id'] }}">

                        {% set cover_img = "/static/videos/animal/" + animal['video']['video_01_folder'] + "/cropped/video-cover.png" %}
                        {% set cover_video = "/static/videos/animal/" + animal['video']['video_01_folder'] + "/cropped/" + animal['video']['video_01_folder'] + ".mp4" %}

                        {#
                        <a href="{{ url_for('full_page_module.full_page', category_name=animal['category']['name_en_slug'], subcategory_name=animal['subcategory']['name_en_slug'], page_url=animal['animal']['page_url'], page_id=animal['animal']['id']) }}"
                           target="_blank" class="cover-wrap">
                            <img class="cover" src={{ config_data['SITE_URL'] }}{{ cover_img }}>
                        </a>
                        #}

                        <div class="cover-wrap"
                             style="background-image: url('{{ config_data['SITE_URL'] }}{{ cover_img }}')">
                            <video id="cover-{{ animal['animal']['id'] }}" class="cover" width="100%" height="100%"
                                   preload="none" muted loop
                                   poster="{{ config_data['SITE_URL'] }}{{ cover_img }}">
                                <source src="{{ config_data['SITE_URL'] }}{{ cover_video }}#t=05" type="video/mp4">
                            </video>

                            <div id="progress-{{ animal['animal']['id'] }}" class="video-progress opacity0">

                                <div class="progress buffer" role="progressbar" aria-label="" aria-valuenow="0"
                                     aria-valuemin="0" aria-valuemax="100">
                                    <div class="progress-bar" style="width: 0%"></div>
                                </div>

                                <div class="progress time" role="progressbar" aria-label="" aria-valuenow="0"
                                     aria-valuemin="0" aria-valuemax="100">
                                    <div class="progress-bar" style="width: 0%"></div>
                                </div>

                            </div>

                            <div class="loader-wrap closed">
                                <div class="loader"></div>
                            </div>

                            <a class="data-link" target="_blank"
                               href="{{ url_for('full_page_module.full_page', category_name=animal['category']['name_en_slug'], subcategory_name=animal['subcategory']['name_en_slug'], page_url=animal['animal']['page_url'], page_id=animal['animal']['id']) }}">
                                <svg xmlns="http://www.w3.org/2000/svg" width="40" height="40" fill="currentColor"
                                     class="bi bi-reply-fill" viewBox="0 0 16 16">
                                    <path d="M5.921 11.9 1.353 8.62a.719.719 0 0 1 0-1.238L5.921 4.1A.716.716 0 0 1 7 4.719V6c1.5 0 6 0 7 8-2.5-4.5-7-4-7-4v1.281c0 .56-.606.898-1.079.62z"/>
                                </svg>
                            </a>

                        </div>

                        <div class="action-wrap">

                            <div class="action-one">
                                <div class="icon">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25"
                                         class="bi bi-person-fill" viewBox="0 0 16 16">
                                        <path d="M3 14s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1H3zm5-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6z"></path>
                                    </svg>
                                </div>
                                <div class="data">
                                    <a target="_blank"
                                       href="{{ url_for('index_module.user_animal_list', user_id=animal['snap']['snap_seller_id'], user_name=animal['snap']['snap_seller_last_name_slug']) }}">{{ animal['snap']['snap_seller_last_name'] }}</a>
                                </div>
                            </div>

                            <div class="action-two">
                                <div class="icon">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25"
                                         class="bi bi-heart-fill" viewBox="0 0 16 16">
                                        <path fill-rule="evenodd"
                                              d="M8 1.314C12.438-3.248 23.534 4.735 8 15-7.534 4.736 3.562-3.248 8 1.314z"></path>
                                    </svg>
                                </div>
                                <div class="data">{{ animal['rating']['rating'] }}</div>
                            </div>

                            <div class="action-three">
                                <div class="icon">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="currentColor"
                                         class="bi bi-question-circle-fill" viewBox="0 0 16 16">
                                        <path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zM5.496 6.033h.825c.138 0 .248-.113.266-.25.09-.656.54-1.134 1.342-1.134.686 0 1.314.343 1.314 1.168 0 .635-.374.927-.965 1.371-.673.489-1.206 1.06-1.168 1.987l.003.217a.25.25 0 0 0 .25.246h.811a.25.25 0 0 0 .25-.25v-.105c0-.718.273-.927 1.01-1.486.609-.463 1.244-.977 1.244-2.056 0-1.511-1.276-2.241-2.673-2.241-1.267 0-2.655.59-2.75 2.286a.237.237 0 0 0 .241.247zm2.325 6.443c.61 0 1.029-.394 1.029-.927 0-.552-.42-.94-1.029-.94-.584 0-1.009.388-1.009.94 0 .533.425.927 1.01.927z"/>
                                    </svg>
                                </div>
                                <div class="data">{{ animal['questions']['questions_count'] }}</div>
                            </div>

                            <div class="action-four">
                                <div class="icon">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="currentColor"
                                         class="bi bi-cup-hot-fill" viewBox="0 0 16 16">
                                        <path fill-rule="evenodd"
                                              d="M.5 6a.5.5 0 0 0-.488.608l1.652 7.434A2.5 2.5 0 0 0 4.104 16h5.792a2.5 2.5 0 0 0 2.44-1.958l.131-.59a3 3 0 0 0 1.3-5.854l.221-.99A.5.5 0 0 0 13.5 6H.5ZM13 12.5a2.01 2.01 0 0 1-.316-.025l.867-3.898A2.001 2.001 0 0 1 13 12.5Z"/>
                                        <path d="m4.4.8-.003.004-.014.019a4.167 4.167 0 0 0-.204.31 2.327 2.327 0 0 0-.141.267c-.026.06-.034.092-.037.103v.004a.593.593 0 0 0 .091.248c.075.133.178.272.308.445l.01.012c.118.158.26.347.37.543.112.2.22.455.22.745 0 .188-.065.368-.119.494a3.31 3.31 0 0 1-.202.388 5.444 5.444 0 0 1-.253.382l-.018.025-.005.008-.002.002A.5.5 0 0 1 3.6 4.2l.003-.004.014-.019a4.149 4.149 0 0 0 .204-.31 2.06 2.06 0 0 0 .141-.267c.026-.06.034-.092.037-.103a.593.593 0 0 0-.09-.252A4.334 4.334 0 0 0 3.6 2.8l-.01-.012a5.099 5.099 0 0 1-.37-.543A1.53 1.53 0 0 1 3 1.5c0-.188.065-.368.119-.494.059-.138.134-.274.202-.388a5.446 5.446 0 0 1 .253-.382l.025-.035A.5.5 0 0 1 4.4.8Zm3 0-.003.004-.014.019a4.167 4.167 0 0 0-.204.31 2.327 2.327 0 0 0-.141.267c-.026.06-.034.092-.037.103v.004a.593.593 0 0 0 .091.248c.075.133.178.272.308.445l.01.012c.118.158.26.347.37.543.112.2.22.455.22.745 0 .188-.065.368-.119.494a3.31 3.31 0 0 1-.202.388 5.444 5.444 0 0 1-.253.382l-.018.025-.005.008-.002.002A.5.5 0 0 1 6.6 4.2l.003-.004.014-.019a4.149 4.149 0 0 0 .204-.31 2.06 2.06 0 0 0 .141-.267c.026-.06.034-.092.037-.103a.593.593 0 0 0-.09-.252A4.334 4.334 0 0 0 6.6 2.8l-.01-.012a5.099 5.099 0 0 1-.37-.543A1.53 1.53 0 0 1 6 1.5c0-.188.065-.368.119-.494.059-.138.134-.274.202-.388a5.446 5.446 0 0 1 .253-.382l.025-.035A.5.5 0 0 1 7.4.8Zm3 0-.003.004-.014.019a4.077 4.077 0 0 0-.204.31 2.337 2.337 0 0 0-.141.267c-.026.06-.034.092-.037.103v.004a.593.593 0 0 0 .091.248c.075.133.178.272.308.445l.01.012c.118.158.26.347.37.543.112.2.22.455.22.745 0 .188-.065.368-.119.494a3.198 3.198 0 0 1-.202.388 5.385 5.385 0 0 1-.252.382l-.019.025-.005.008-.002.002A.5.5 0 0 1 9.6 4.2l.003-.004.014-.019a4.149 4.149 0 0 0 .204-.31 2.06 2.06 0 0 0 .141-.267c.026-.06.034-.092.037-.103a.593.593 0 0 0-.09-.252A4.334 4.334 0 0 0 9.6 2.8l-.01-.012a5.099 5.099 0 0 1-.37-.543A1.53 1.53 0 0 1 9 1.5c0-.188.065-.368.119-.494.059-.138.134-.274.202-.388a5.446 5.446 0 0 1 .253-.382l.025-.035A.5.5 0 0 1 10.4.8Z"/>
                                    </svg>
                                </div>
                                <div class="data">{{ animal['talking']['talking_count'] }}</div>
                            </div>

                            <div class="action-five">
                                <div class="icon">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="currentColor"
                                         class="bi bi-currency-exchange" viewBox="0 0 16 16">
                                        <path d="M0 5a5.002 5.002 0 0 0 4.027 4.905 6.46 6.46 0 0 1 .544-2.073C3.695 7.536 3.132 6.864 3 5.91h-.5v-.426h.466V5.05c0-.046 0-.093.004-.135H2.5v-.427h.511C3.236 3.24 4.213 2.5 5.681 2.5c.316 0 .59.031.819.085v.733a3.46 3.46 0 0 0-.815-.082c-.919 0-1.538.466-1.734 1.252h1.917v.427h-1.98c-.003.046-.003.097-.003.147v.422h1.983v.427H3.93c.118.602.468 1.03 1.005 1.229a6.5 6.5 0 0 1 4.97-3.113A5.002 5.002 0 0 0 0 5zm16 5.5a5.5 5.5 0 1 1-11 0 5.5 5.5 0 0 1 11 0zm-7.75 1.322c.069.835.746 1.485 1.964 1.562V14h.54v-.62c1.259-.086 1.996-.74 1.996-1.69 0-.865-.563-1.31-1.57-1.54l-.426-.1V8.374c.54.06.884.347.966.745h.948c-.07-.804-.779-1.433-1.914-1.502V7h-.54v.629c-1.076.103-1.808.732-1.808 1.622 0 .787.544 1.288 1.45 1.493l.358.085v1.78c-.554-.08-.92-.376-1.003-.787H8.25zm1.96-1.895c-.532-.12-.82-.364-.82-.732 0-.41.311-.719.824-.809v1.54h-.005zm.622 1.044c.645.145.943.38.943.796 0 .474-.37.8-1.02.86v-1.674l.077.018z"/>
                                    </svg>
                                </div>

                                <div class="data">
