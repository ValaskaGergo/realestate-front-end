{% extends "layout.jinja2" %}

{# Start Breadcrumb #}
{% block breadcrumb %}
    <div id="breadcrumb">

        <div class="container w-100 mw-100 pl-3 pr-3">
            <div class="row">
                <div class="col-12">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a class="hover" href="{{ url_for('index_module.index') }}"
                                                           data-i18n="anlihouse-anlihouse"></a>
                            </li>
                            <li class="breadcrumb-item"><a class="hover" href="{{ url_for('index_module.index') }}"
                                                           data-i18n="anlihouse-anlibreeders"></a></li>
                            <li class="breadcrumb-item active" aria-current="page" data-i18n="kategoriaKezeles"></li>
                        </ol>
                    </nav>
                </div>
            </div>
        </div>

    </div>
{% endblock %}
{# End Breadcrumb #}

{# Start Content #}
{% block content %}
    <div class="container">
        <div class="row">
            <div class="col-12">

                {# Start Category #}
                <div class="card CategoryWrap">
                    <div class="card-header">
                        <span data-i18n="kategoriak"></span>
                        <span id="addCategoryIcon" class="card-header-icon cursor-pointer plus-icon inactive"
                              data-toggle="modal"
                              data-target="#addCategory">
                                            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                                 fill="currentColor" class="bi bi-plus-circle-fill" viewBox="0 0 16 16"><path
                                                    d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zM8.5 4.5a.5.5 0 0 0-1 0v3h-3a.5.5 0 0 0 0 1h3v3a.5.5 0 0 0 1 0v-3h3a.5.5 0 0 0 0-1h-3v-3z"/>
                                            </svg>
                        </span>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive table-category">
                            <table class="table table-striped">
                                <thead>
                                <tr>
                                    <th scope="col" data-i18n="id"></th>
                                    <th scope="col" data-i18n="kategoria"></th>
                                    <th scope="col" data-i18n="kep"></th>
                                    <th scope="col" data-i18n="lathato"></th>
                                    <th scope="col" data-i18n="utolso_mod_user"></th>
                                    <th scope="col" data-i18n="utolso_mod_ideje"></th>
                                    <th scope="col"></th>
                                    <th scope="col"></th>
                                </tr>
                                </thead>
                                <tbody>
                                {% for category in category_data %}
                                    {% if lang == "hu" %}
                                        {% set name = category.name_hu %}
                                    {% elif lang == "en" %}
                                        {% set name = category.name_en %}
                                    {% elif lang == "de" %}
                                        {% set name = category.name_de %}
                                    {% elif lang == "fr" %}
                                        {% set name = category.name_fr %}
                                    {% elif lang == "es" %}
                                        {% set name = category.name_es %}
                                    {% endif %}
                                    <tr data-category-id="{{ category.category_id }}">
                                        <th scope="row">#{{ category.category_id }}</th>
                                        <td>{{ name }}</td>
                                        <td>
                                            {% if category.img != None %}
                                                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20"
                                                     class="bi bi-check-lg" viewBox="0 0 16 16">
                                                    <path d="M12.736 3.97a.733.733 0 0 1 1.047 0c.286.289.29.756.01 1.05L7.88 12.01a.733.733 0 0 1-1.065.02L3.217 8.384a.757.757 0 0 1 0-1.06.733.733 0 0 1 1.047 0l3.052 3.093 5.4-6.425a.247.247 0 0 1 .02-.022Z"/>
                                                </svg>
                                            {% else %}
                                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24"
                                                     fill="#dc3545" class="bi bi-x" viewBox="0 0 16 16">
                                                    <path d="M4.646 4.646a.5.5 0 0 1 .708 0L8 7.293l2.646-2.647a.5.5 0 0 1 .708.708L8.707 8l2.647 2.646a.5.5 0 0 1-.708.708L8 8.707l-2.646 2.647a.5.5 0 0 1-.708-.708L7.293 8 4.646 5.354a.5.5 0 0 1 0-.708z"/>
                                                </svg>
                                            {% endif %}
                                        </td>
                                        <td>
                                            {% if category.visibility == "True" %}
                                                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20"
                                                     class="bi bi-check-lg" viewBox="0 0 16 16">
                                                    <path d="M12.736 3.97a.733.733 0 0 1 1.047 0c.286.289.29.756.01 1.05L7.88 12.01a.733.733 0 0 1-1.065.02L3.217 8.384a.757.757 0 0 1 0-1.06.733.733 0 0 1 1.047 0l3.052 3.093 5.4-6.425a.247.247 0 0 1 .02-.022Z"/>
                                                </svg>
                                            {% else %}
                                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24"
                                                     fill="#dc3545" class="bi bi-x" viewBox="0 0 16 16">
                                                    <path d="M4.646 4.646a.5.5 0 0 1 .708 0L8 7.293l2.646-2.647a.5.5 0 0 1 .708.708L8.707 8l2.647 2.646a.5.5 0 0 1-.708.708L8 8.707l-2.646 2.647a.5.5 0 0 1-.708-.708L7.293 8 4.646 5.354a.5.5 0 0 1 0-.708z"/>
                                                </svg>
                                            {% endif %}
                                        </td>
                                        <td>
                                            <a href="{{ url_for('user_management_module.user_management_user_id', user_id=category.category_user_id) }}"
                                               target="_blank">{{ category.category_user_name }}</a>
                                        </td>
                                        <td data-time-llll="{{ category.updated_at }}">{{ category.updated_at }}</td>
                                        <td class="text-center">
                                            <span class="icon-click edit-category" data-id="{{ category.category_id }}">
                                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                                     fill="currentColor" class="bi bi-pencil-fill" viewBox="0 0 16 16">
                                                <path d="M12.854.146a.5.5 0 0 0-.707 0L10.5 1.793 14.207 5.5l1.647-1.646a.5.5 0 0 0 0-.708l-3-3zm.646 6.061L9.793 2.5 3.293 9H3.5a.5.5 0 0 1 .5.5v.5h.5a.5.5 0 0 1 .5.5v.5h.5a.5.5 0 0 1 .5.5v.5h.5a.5.5 0 0 1 .5.5v.207l6.5-6.5zm-7.468 7.468A.5.5 0 0 1 6 13.5V13h-.5a.5.5 0 0 1-.5-.5V12h-.5a.5.5 0 0 1-.5-.5V11h-.5a.5.5 0 0 1-.5-.5V10h-.5a.499.499 0 0 1-.175-.032l-.179.178a.5.5 0 0 0-.11.168l-2 5a.5.5 0 0 0 .65.65l5-2a.5.5 0 0 0 .168-.11l.178-.178z"/>
                                            </svg>
                                            </span>
                                        </td>

                                        {% if category.subcategory_count == 0 %}
                                            <td class="text-center">
                                            <span class="category-toggle-x" data-id="{{ category.category_id }}"
                                                  style="cursor:default">
                                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24"
                                                     fill="#dc3545" class="bi bi-x" viewBox="0 0 16 16">
                                                    <path d="M4.646 4.646a.5.5 0 0 1 .708 0L8 7.293l2.646-2.647a.5.5 0 0 1 .708.708L8.707 8l2.647 2.646a.5.5 0 0 1-.708.708L8 8.707l-2.646 2.647a.5.5 0 0 1-.708-.708L7.293 8 4.646 5.354a.5.5 0 0 1 0-.708z"/>
                                                </svg>
                                            </span>
                                            </td>
                                        {% elif category.subcategory_count != 0 and category.category_id == toggle_id %}
                                            <td class="text-center">
                                            <span class="category-toggle-on" data-id="{{ category.category_id }}"
                                                  data-subcategory-run-id="{{ category.category_id }}"
                                                  style="cursor:default">
                                                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20"
                                                     class="bi bi-toggle-on" viewBox="0 0 16 16">
                                                    <path d="M5 3a5 5 0 0 0 0 10h6a5 5 0 0 0 0-10H5zm6 9a4 4 0 1 1 0-8 4 4 0 0 1 0 8z"/>
                                                </svg>
                                            </span>
                                            </td>
                                        {% else %}
                                            <td class="text-center">
                                            <span class="icon-click category-toggle-off"
                                                  data-id="{{ category.category_id }}">
                                                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20"
                                                     fill="currentColor" class="bi bi-toggle-off" viewBox="0 0 16 16">
                                                    <path d="M11 4a4 4 0 0 1 0 8H8a4.992 4.992 0 0 0 2-4 4.992 4.992 0 0 0-2-4h3zm-6 8a4 4 0 1 1 0-8 4 4 0 0 1 0 8zM0 8a5 5 0 0 0 5 5h6a5 5 0 0 0 0-10H5a5 5 0 0 0-5 5z"/>
                                                </svg>
                                            </span>
                                            </td>
                                        {% endif %}

                                    </tr>
                                {% endfor %}
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <div class="card-footer text-end" data-i18n="anlihouse-anlibreeders">
                    </div>
                </div>
                {# End Category #}

                {# Start Sub-category #}
                <div id="SubCategoryWrap" class="card SubCategoryWrap mt-5">
                    <div class="card-header">
                        <span data-i18n="alkategoriak"></span>
                        {% if category_data != [] %}
                            <span id="addSubCategoryIcon" class="card-header-icon cursor-pointer plus-icon inactive"
                                  data-toggle="modal"
                                  data-target="#addSettings">
                                            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                                 fill="currentColor" class="bi bi-plus-circle-fill" viewBox="0 0 16 16"><path
                                                    d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zM8.5 4.5a.5.5 0 0 0-1 0v3h-3a.5.5 0 0 0 0 1h3v3a.5.5 0 0 0 1 0v-3h3a.5.5 0 0 0 0-1h-3v-3z"/>
                                            </svg>
                        </span>
                        {% endif %}
                    </div>
                    <div class="card-body">
                        <div class="table-responsive table-subcategory">
                            <table class="table table-striped">
                                <thead>
                                <tr>
                                    <th scope="col" data-i18n="id"></th>
                                    <th scope="col" data-i18n="alkategoria"></th>
                                    <th scope="col" data-i18n="kep"></th>
                                    <th scope="col" data-i18n="lathato"></th>
                                    <th scope="col" data-i18n="utolso_mod_user"></th>
                                    <th scope="col" data-i18n="utolso_mod_ideje"></th>
                                    <th scope="col"></th>
                                </tr>
                                </thead>
                                <tbody class="tbody-subcategory">
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <div class="card-footer text-end" data-i18n="anlihouse-anlibreeders">
                    </div>
                </div>
                {# End Sub-Category #}

            </div>
        </div>
    </div>

    {# Start Category Modal #}
    <div class="modal fade" id="addCategory" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <span data-i18n="uj-kategoria"></span>

                    <div class="modal-close cursor-pointer">
                        <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="currentColor"
                             class="bi bi-x-lg" viewBox="0 0 16 16">
                            <path fill-rule="evenodd"
                                  d="M13.854 2.146a.5.5 0 0 1 0 .708l-11 11a.5.5 0 0 1-.708-.708l11-11a.5.5 0 0 1 .708 0Z"/>
                            <path fill-rule="evenodd"
                                  d="M2.146 2.146a.5.5 0 0 0 0 .708l11 11a.5.5 0 0 0 .708-.708l-11-11a.5.5 0 0 0-.708 0Z"/>
                        </svg>
                    </div>
                </div>
                <div class="modal-body">

                    <form id="add-category-form" onsubmit="return false" novalidate="novalidate">
                        {{ add_category_form.csrf_token }}

                        {{ add_category_form.category_id(type='hidden', id='categoryId', class='form-control') }}
                        {{ add_category_form.visibility(type='hidden', id='categoryVisibility', class='form-control', value="True") }}
                        {{ add_category_form.img_data(type='hidden', id='categoryImgData', class='form-control') }}
                        {{ add_category_form.img_data_old(type='hidden', id='categoryImgDataOld', class='form-control') }}
                        {{ add_category_form.is_edit_img(type='hidden', id='categoryIsEditImg', class='form-control') }}
                        {{ add_category_form.is_new_img(type='hidden', id='categoryIsNewImg', class='form-control') }}
                        {{ add_category_form.is_rm_img(type='hidden', id='categoryIsRmImg', class='form-control') }}

                        <div class="alert alert-danger form-alert-danger mb-3 d-none" role="alert"
                             data-i18n="anlihouse-A21">
                        </div>

                        {# Start visibility checkbox #}
                        <div class="visibility mb-3">
                            <input class="category-visibility-checkbox" type="checkbox" id="CategoryCheckboxVisibility"
                                   name="checkbox" checked="checked">
                            <label for="CategoryCheckboxVisibility" class="d-content" data-i18n="lathato"
                                   style="display:contents;"></label>
                        </div>
                        {# End visibility checkbox #}

                        <fieldset>
                            <legend class="w-auto" style="font-size:1.2rem;font-weight:500;"
                                    data-i18n="kategoria"></legend>

                            {# Start add_category_form.name_hu #}
                            <div class="mb-4">
                                <label for="categoryNameHU" class="form-label">
                                    <span data-i18n="anlihouse-HU"></span>
                                    <span class="text-danger text-lowercase fs-12px"
                                          data-i18n="anlihouse-A18"></span>
                                </label>
                                <span class="input-group">
                                {{ add_category_form.name_hu(type='text', id='categoryNameHU', class='form-control') }}
                                <span class="input-group-text cursor-pointer translate-hu translate-icon inactive"
                                      data-translate="HU">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="22" height="22" fill="currentColor"
                                         class="bi bi-translate" viewBox="0 0 16 16">
                                        <path d="M4.545 6.714 4.11 8H3l1.862-5h1.284L8 8H6.833l-.435-1.286H4.545zm1.634-.736L5.5 3.956h-.049l-.679 2.022H6.18z"/>
                                        <path d="M0 2a2 2 0 0 1 2-2h7a2 2 0 0 1 2 2v3h3a2 2 0 0 1 2 2v7a2 2 0 0 1-2 2H7a2 2 0 0 1-2-2v-3H2a2 2 0 0 1-2-2V2zm2-1a1 1 0 0 0-1 1v7a1 1 0 0 0 1 1h7a1 1 0 0 0 1-1V2a1 1 0 0 0-1-1H2zm7.138 9.995c.193.301.402.583.63.846-.748.575-1.673 1.001-2.768 1.292.178.217.451.635.555.867 1.125-.359 2.08-.844 2.886-1.494.777.665 1.739 1.165 2.93 1.472.133-.254.414-.673.629-.89-1.125-.253-2.057-.694-2.82-1.284.681-.747 1.222-1.651 1.621-2.757H14V8h-3v1.047h.765c-.318.844-.74 1.546-1.272 2.13a6.066 6.066 0 0 1-.415-.492 1.988 1.988 0 0 1-.94.31z"/>
                                    </svg>
                                </span>
                            </span>
                                <small class="text-danger input-error add-category-name-hu-error d-none">
                                    <span data-i18n=""></span>
                                </small>
                            </div>
                            {# End add_category_form.name_hu #}

                            {# Start add_category_form.name_en #}
                            <div class="mb-4">
                                <label for="categoryNameEN" class="form-label">
                                    <span data-i18n="anlihouse-EN"></span>
                                    <span class="text-danger text-lowercase fs-12px"
                                          data-i18n="anlihouse-A18"></span>
                                </label>
                                <span class="input-group">
                                {{ add_category_form.name_en(type='text', id='categoryNameEN', class='form-control') }}
                                <span class="input-group-text cursor-pointer translate-en translate-icon inactive"
                                      data-translate="EN">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="22" height="22" fill="currentColor"
                                         class="bi bi-translate" viewBox="0 0 16 16">
                                        <path d="M4.545 6.714 4.11 8H3l1.862-5h1.284L8 8H6.833l-.435-1.286H4.545zm1.634-.736L5.5 3.956h-.049l-.679 2.022H6.18z"/>
                                        <path d="M0 2a2 2 0 0 1 2-2h7a2 2 0 0 1 2 2v3h3a2 2 0 0 1 2 2v7a2 2 0 0 1-2 2H7a2 2 0 0 1-2-2v-3H2a2 2 0 0 1-2-2V2zm2-1a1 1 0 0 0-1 1v7a1 1 0 0 0 1 1h7a1 1 0 0 0 1-1V2a1 1 0 0 0-1-1H2zm7.138 9.995c.193.301.402.583.63.846-.748.575-1.673 1.001-2.768 1.292.178.217.451.635.555.867 1.125-.359 2.08-.844 2.886-1.494.777.665 1.739 1.165 2.93 1.472.133-.254.414-.673.629-.89-1.125-.253-2.057-.694-2.82-1.284.681-.747 1.222-1.651 1.621-2.757H14V8h-3v1.047h.765c-.318.844-.74 1.546-1.272 2.13a6.066 6.066 0 0 1-.415-.492 1.988 1.988 0 0 1-.94.31z"/>
                                    </svg>
                                </span>
                            </span>
                                <small class="text-danger input-error add-category-name-en-error d-none">
                                    <span data-i18n=""></span>
                                </small>
                            </div>
                            {# End add_category_form.name_en #}

                            {# Start add_category_form.name_de #}
                            <div class="mb-4">
                                <label for="categoryNameDE" class="form-label">
                                    <span data-i18n="anlihouse-DE"></span>
                                    <span class="text-danger text-lowercase fs-12px"
                                          data-i18n="anlihouse-A18"></span>
                                </label>
                                <span class="input-group">
                                {{ add_category_form.name_de(type='text', id='categoryNameDE', class='form-control') }}
                                <span class="input-group-text cursor-pointer translate-de translate-icon inactive"
                                      data-translate="DE">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="22" height="22" fill="currentColor"
                                         class="bi bi-translate" viewBox="0 0 16 16">
                                        <path d="M4.545 6.714 4.11 8H3l1.862-5h1.284L8 8H6.833l-.435-1.286H4.545zm1.634-.736L5.5 3.956h-.049l-.679 2.022H6.18z"/>
                                        <path d="M0 2a2 2 0 0 1 2-2h7a2 2 0 0 1 2 2v3h3a2 2 0 0 1 2 2v7a2 2 0 0 1-2 2H7a2 2 0 0 1-2-2v-3H2a2 2 0 0 1-2-2V2zm2-1a1 1 0 0 0-1 1v7a1 1 0 0 0 1 1h7a1 1 0 0 0 1-1V2a1 1 0 0 0-1-1H2zm7.138 9.995c.193.301.402.583.63.846-.748.575-1.673 1.001-2.768 1.292.178.217.451.635.555.867 1.125-.359 2.08-.844 2.886-1.494.777.665 1.739 1.165 2.93 1.472.133-.254.414-.673.629-.89-1.125-.253-2.057-.694-2.82-1.284.681-.747 1.222-1.651 1.621-2.757H14V8h-3v1.047h.765c-.318.844-.74 1.546-1.272 2.13a6.066 6.066 0 0 1-.415-.492 1.988 1.988 0 0 1-.94.31z"/>
                                    </svg>
                                </span>
                            </span>
                                <small class="text-danger input-error add-category-name-de-error d-none">
                                    <span data-i18n=""></span>
                                </small>
                            </div>
                            {# End add_category_form.name_de #}

                            {# Start add_category_form.name_fr #}
                            <div class="mb-4">
                                <label for="categoryNameFR" class="form-label">
                                    <span data-i18n="anlihouse-FR"></span>
                                    <span class="text-danger text-lowercase fs-12px"
                                          data-i18n="anlihouse-A18"></span>
                                </label>
                                <span class="input-group">
                                {{ add_category_form.name_fr(type='text', id='categoryNameFR', class='form-control') }}
                                <span class="input-group-text cursor-pointer translate-fr translate-icon inactive"
                                      data-translate="FR">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="22" height="22" fill="currentColor"
                                         class="bi bi-translate" viewBox="0 0 16 16">
                                        <path d="M4.545 6.714 4.11 8H3l1.862-5h1.284L8 8H6.833l-.435-1.286H4.545zm1.634-.736L5.5 3.956h-.049l-.679 2.022H6.18z"/>
                                        <path d="M0 2a2 2 0 0 1 2-2h7a2 2 0 0 1 2 2v3h3a2 2 0 0 1 2 2v7a2 2 0 0 1-2 2H7a2 2 0 0 1-2-2v-3H2a2 2 0 0 1-2-2V2zm2-1a1 1 0 0 0-1 1v7a1 1 0 0 0 1 1h7a1 1 0 0 0 1-1V2a1 1 0 0 0-1-1H2zm7.138 9.995c.193.301.402.583.63.846-.748.575-1.673 1.001-2.768 1.292.178.217.451.635.555.867 1.125-.359 2.08-.844 2.886-1.494.777.665 1.739 1.165 2.93 1.472.133-.254.414-.673.629-.89-1.125-.253-2.057-.694-2.82-1.284.681-.747 1.222-1.651 1.621-2.757H14V8h-3v1.047h.765c-.318.844-.74 1.546-1.272 2.13a6.066 6.066 0 0 1-.415-.492 1.988 1.988 0 0 1-.94.31z"/>
                                    </svg>
                                </span>
                            </span>
                                <small class="text-danger input-error add-category-name-fr-error d-none">
                                    <span data-i18n=""></span>
                                </small>
                            </div>
                            {# End add_category_form.name_fr #}

                            {# Start add_category_form.name_es #}
                            <div class="mb-3">
                                <label for="categoryNameES" class="form-label">
                                    <span data-i18n="anlihouse-ES"></span>
                                    <span class="text-danger text-lowercase fs-12px"
                                          data-i18n="anlihouse-A18"></span>
                                </label>
                                <span class="input-group">
                                {{ add_category_form.name_es(type='text', id='categoryNameES', class='form-control') }}
                                <span class="input-group-text cursor-pointer translate-es translate-icon inactive"
                                      data-translate="ES">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="22" height="22" fill="currentColor"
