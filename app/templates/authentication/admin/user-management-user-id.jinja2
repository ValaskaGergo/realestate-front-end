{% extends "layout.jinja2" %}

{# Start Breadcrumb #}
{% block breadcrumb %}
    <div id="breadcrumb" data-inactive="{{ inactive_account }}" data-deleted="{{ deleted }}">

        <div class="container w-100 mw-100 pl-3 pr-3">
            <div class="row">
                <div class="col-12">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a class="hover" href="{{ url_for('index_module.index') }}"
                                                           data-i18n="anlihouse-anlihouse"></a></li>
                            <li class="breadcrumb-item"><a class="hover" href="{{ url_for('index_module.index') }}"
                                                           data-i18n="anlihouse-anlibreeders"></a></li>
                            {% if inactive_account == "False" and deleted == "False" %}
                                <li class="breadcrumb-item"><a class="hover previous-click"
                                                               href="{{ url_for('user_management_module.user_management') }}"
                                                               data-i18n="anlihouse-A219"></a></li>
                            {% elif inactive_account == "True" and deleted == "False" %}
                                <li class="breadcrumb-item"><a class="hover previous-click"
                                                               href="{{ url_for('user_management_module.user_management_inactive') }}"
                                                               data-i18n="anlihouse-A220"></a></li>
                            {% else %}
                                <li class="breadcrumb-item"><a class="hover previous-click"
                                                               href="{{ url_for('user_management_module.user_management_deleted') }}"
                                                               data-i18n="anlihouse-A221"></a></li>
                            {% endif %}
                            <li class="breadcrumb-item active" aria-current="page">{{ first_name }} {{ last_name }}</li>
                        </ol>
                    </nav>
                </div>
            </div>
        </div>

    </div>
{% endblock %}
{# End Breadcrumb #}

{# Start Content #}
{% block content %}
    <div class="container billingAndShippingAddress">
        <div class="row">
            <div class="col-12">

                <form id="billing-address-form" onsubmit="return false" novalidate="novalidate">
                    <div class="card">
                        <div class="card-header" data-i18n="anlihouse-A65"></div>
                        <div class="card-body">

                            {{ billing_form.csrf_token }}

                            {{ billing_form.user_email(type='hidden', id='billingAddressUserEmail', class='form-control') }}
                            {{ billing_form.billing_is_company(type='hidden', id='billingAddressIsCompany', class='form-control') }}
                            {{ billing_form.billing_is_shipping_address(type='hidden', id='billingAddressIsShipping', class='form-control') }}
                            {{ billing_form.billing_country(type='hidden', id='billingAddressCountry', class='form-control') }}
                            {{ billing_form.billing_currency(type='hidden', id='billingAddressCurrency', class='form-control') }}

                            {{ shipping_form.shipping_country(type='hidden', id='shippingAddressCountry', class='form-control') }}
