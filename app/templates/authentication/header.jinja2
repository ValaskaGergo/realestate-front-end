<div id="header" class="pt-3 pb-3">

    <div class="container w-100 mw-100 pl-3 pr-3">
        <div class="row">
            <div class="col-12">

                <a href="{{ url_for('index_module.index') }}" id="header-logo" class="">
                    <img class="img" src="{{ url_for("static", filename="images/layout/anli_breeders_logo.png") }}"
                         alt="ANLI Breeders"/>
                    <span class="text" data-i18n="anlihouse-anlibreeders"></span>
                </a>

                <div id="hamburger" class="closed user-select-none">
                    <span class="hamburger-line line01"></span>
                    <span class="hamburger-line line02"></span>
                    <span class="hamburger-line line03"></span>
                    <span class="hamburger-line line04"></span>
                    <span class="hamburger-text" data-i18n="anlihouse-A01"></span>
                </div>

            </div>
        </div>
    </div>

</div>

<div id="search">
    <div class="container w-100 mw-100 pl-3 pr-3">
        <div class="row">
            <div class="col-12">

                <div class="search auth">

                    <div class="search-icon">
                        <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="currentColor"
                             class="bi bi-search" viewBox="0 0 16 16">
                            <path d="M11.742 10.344a6.5 6.5 0 1 0-1.397 1.398h-.001c.03.04.062.078.098.115l3.85 3.85a1 1 0 0 0 1.415-1.414l-3.85-3.85a1.007 1.007 0 0 0-.115-.1zM12 6.5a5.5 5.5 0 1 1-11 0 5.5 5.5 0 0 1 11 0z"/>
                        </svg>
                    </div>

                    <div class="search-text">
                        <form id="filter-form" onsubmit="return false" novalidate="novalidate">
                            {{ filter_form.csrf_token }}

                            <input type="hidden" id="room" name="room"
                                   value="{{ is_user_login['user_profile']['username'] }}" readonly>

                            {{ filter_form.where_input(type='text', id='searchInput', class='form-control') }}

                            {{ filter_form.where_all(type='hidden', id='filterWhereAllInput', class='form-control') }}

                            {{ filter_form.where_name(type='hidden', id='filterWhereNameInput', class='form-control') }}

                            {{ filter_form.where_description(type='hidden', id='filterWhereDescriptionInput', class='form-control') }}

                            {{ filter_form.where_family(type='hidden', id='filterWhereFamilyInput', class='form-control') }}
                            {{ filter_form.where_family_mother(type='hidden', id='filterWhereFamilyMotherInput', class='form-control') }}
                            {{ filter_form.where_family_mother_mother(type='hidden', id='filterWhereFamilyMotherMotherInput', class='form-control') }}
                            {{ filter_form.where_family_mother_father(type='hidden', id='filterWhereFamilyMotherFatherInput', class='form-control') }}
                            {{ filter_form.where_family_mother_mother_mother(type='hidden', id='filterWhereFamilyMotherMotherMotherInput', class='form-control') }}
                            {{ filter_form.where_family_mother_mother_father(type='hidden', id='filterWhereFamilyMotherMotherFatherInput', class='form-control') }}
                            {{ filter_form.where_family_mother_father_mother(type='hidden', id='filterWhereFamilyMotherFatherMotherInput', class='form-control') }}
                            {{ filter_form.where_family_mother_father_father(type='hidden', id='filterWhereFamilyMotherFatherFatherInput', class='form-control') }}
                            {{ filter_form.where_family_father(type='hidden', id='filterWhereFamilyFatherInput', class='form-control') }}
                            {{ filter_form.where_family_father_mother(type='hidden', id='filterWhereFamilyFatherMotherInput', class='form-control') }}
                            {{ filter_form.where_family_father_father(type='hidden', id='filterWhereFamilyFatherFatherInput', class='form-control') }}
                            {{ filter_form.where_family_father_mother_mother(type='hidden', id='filterWhereFamilyFatherMotherMotherInput', class='form-control') }}
                            {{ filter_form.where_family_father_mother_father(type='hidden', id='filterWhereFamilyFatherMotherFatherInput', class='form-control') }}
                            {{ filter_form.where_family_father_father_mother(type='hidden', id='filterWhereFamilyFatherFatherMotherInput', class='form-control') }}
                            {{ filter_form.where_family_father_father_father(type='hidden', id='filterWhereFamilyFatherFatherFatherInput', class='form-control') }}

                            {{ filter_form.price_min(type='hidden', id='filterPriceMinInput', class='form-control') }}
                            {{ filter_form.price_max(type='hidden', id='filterPriceMaxInput', class='form-control') }}

                            {{ filter_form.category_all(type='hidden', id='filterCategoryInput', class='form-control') }}

                            {{ filter_form.subcategory_all(type='hidden', id='filterSubcategoryInput', class='form-control') }}

                            {{ filter_form.gender_all(type='hidden', id='filterGenderInput', class='form-control') }}

                            {{ filter_form.color_all(type='hidden', id='filterColorInput', class='form-control') }}

                            {{ filter_form.be_used_for_all(type='hidden', id='filterBeUsedForInput', class='form-control') }}

                            {{ filter_form.age_min(type='hidden', id='filterAgeMinInput', class='form-control') }}
                            {{ filter_form.age_max(type='hidden', id='filterAgeMaxInput', class='form-control') }}

                            {{ filter_form.region_residence(type='hidden', id='filterRegionResidenceInput', class='form-control') }}
                            {{ filter_form.country_residence(type='hidden', id='filterCountryResidenceInput', class='form-control') }}

                            {{ filter_form.region_origin(type='hidden', id='filterRegionOriginInput', class='form-control') }}
                            {{ filter_form.country_origin(type='hidden', id='filterCountryOriginInput', class='form-control') }}

                            {{ filter_form.order_by_price(type='hidden', id='filterOrderByPrice', class='form-control') }}
                            {{ filter_form.order_by_age(type='hidden', id='filterOrderByAge', class='form-control') }}
                            {{ filter_form.order_by_rating(type='hidden', id='filterOrderByRating', class='form-control') }}

                            {{ filter_form.encode(type='hidden', id='filterEncode', class='form-control') }}

                            {{ filter_form.only_one_category_id(type='hidden', id='filterOnlyOneCategoryId', class='form-control') }}
                            {{ filter_form.only_one_subcategory_id(type='hidden', id='filterOnlyOneSubcategoryId', class='form-control') }}

                            {{ filter_form.seller_user_id(type='hidden', id='filterSellerUserId', class='form-control') }}
                            {{ filter_form.seller_user_name(type='hidden', id='filterSellerUserName', class='form-control') }}

                            {{ filter_form.wishlist(type='hidden', id='filterWishlist', class='form-control') }}

                            {{ filter_form.snap(type='hidden', id='filterSnap', class='form-control') }}
                        </form>
                    </div>

                    <div class="search-close inactive">
                        <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30"
                             class="bi bi-x" viewBox="0 0 16 16">
                            <path d="M4.646 4.646a.5.5 0 0 1 .708 0L8 7.293l2.646-2.647a.5.5 0 0 1 .708.708L8.707 8l2.647 2.646a.5.5 0 0 1-.708.708L8 8.707l-2.646 2.647a.5.5 0 0 1-.708-.708L7.293 8 4.646 5.354a.5.5 0 0 1 0-.708z"/>
                        </svg>
                    </div>

                </div>

                <div id="search-user-info" class="closed">
                    <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="currentColor"
                         class="bi bi-person-fill" viewBox="0 0 16 16">
                        <path d="M3 14s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1H3zm5-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6z"/>
                    </svg>

                    <div id="userinfo-wrap" class="closed">
                        <div id="userinfo" class="shadow">
                            <div class="container w-100 mw-100 pl-3 pr-3">
                                <div class="row">
                                    <div class="col-12">

                                        <div class="loader-wrap closed">
                                            <span class="loader"></span>
                                        </div>

                                        <div id="userinfo-container" class="d-none">
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>

                <div id="search-user-notification" class="closed">
                    {% if is_user_login['messages_received']['received'] != 0 %}
                        <span class="badge bell">{{ is_user_login['messages_received']['received'] }}</span>
                    {% else %}
                        <span class="badge bell "
                              style="display:none;">{{ is_user_login['messages_received']['received'] }}</span>
                    {% endif %}
                    <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="currentColor"
                         class="bi bi-bell-fill" viewBox="0 0 16 16">
                        <path d="M8 16a2 2 0 0 0 2-2H6a2 2 0 0 0 2 2zm.995-14.901a1 1 0 1 0-1.99 0A5.002 5.002 0 0 0 3 6c0 1.098-.5 6-2 7h14c-1.5-1-2-5.902-2-7 0-2.42-1.72-4.44-4.005-4.901z"/>
                    </svg>

                    <div id="notification-wrap" class="closed">
                        <div id="notification" class="shadow">
                            <div class="container w-100 mw-100 pl-3 pr-3">
                                <div class="row">
                                    <div class="col-12">

                                        <div class="loader-wrap closed">
                                            <span class="loader"></span>
                                        </div>

                                        <div id="notification-container" class="d-none">

                                            <div id="head">
                                                <div id="title" data-i18n="anlihouse-A233"></div>
                                                <div id="settings" data-bs-toggle="dropdown">
                                                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16"
                                                         fill="currentColor" class="bi bi-gear-fill"
                                                         viewBox="0 0 16 16">
                                                        <path d="M9.405 1.05c-.413-1.4-2.397-1.4-2.81 0l-.1.34a1.464 1.464 0 0 1-2.105.872l-.31-.17c-1.283-.698-2.686.705-1.987 1.987l.169.311c.446.82.023 1.841-.872 2.105l-.34.1c-1.4.413-1.4 2.397 0 2.81l.34.1a1.464 1.464 0 0 1 .872 2.105l-.17.31c-.698 1.283.705 2.686 1.987 1.987l.311-.169a1.464 1.464 0 0 1 2.105.872l.1.34c.413 1.4 2.397 1.4 2.81 0l.1-.34a1.464 1.464 0 0 1 2.105-.872l.31.17c1.283.698 2.686-.705 1.987-1.987l-.169-.311a1.464 1.464 0 0 1 .872-2.105l.34-.1c1.4-.413 1.4-2.397 0-2.81l-.34-.1a1.464 1.464 0 0 1-.872-2.105l.17-.31c.698-1.283-.705-2.686-1.987-1.987l-.311.169a1.464 1.464 0 0 1-2.105-.872l-.1-.34zM8 10.93a2.929 2.929 0 1 1 0-5.86 2.929 2.929 0 0 1 0 5.858z"/>
                                                    </svg>
                                                </div>
                                                <ul class="dropdown-menu shadow settings-ul mt-2">
                                                    <li class="dropdown-item"><a
                                                            href="{{ url_for('notification_module.notifications') }}"
                                                            data-i18n="anlihouse-A403"></a></li>
                                                </ul>
                                            </div>

                                            <div id="content-list">
                                            </div>

                                            <div id="chat-list-additional" class="d-none"
                                                 data-i18n="anlihouse-A241"></div>

                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>

            </div>
        </div>
    </div>

    <div class="container w-100 mw-100 pl-3 pr-3">
        <div class="row">
            <div class="col-12">
                <div id="filter-wrap" class="inactive">
                    <div id="filter">

