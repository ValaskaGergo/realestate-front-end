                                    </svg>
                                </span>

                                {% if is_user_login['user_permission']['is_admin'] == "True" or is_user_login['user_permission']['subscribed'] == "True" %}
                                    <div id="price-slider"></div>
                                {% else %}
                                    {# <div id="price-slider" disabled="false"></div> #}
                                    <div id="price-slider"></div>
                                {% endif %}
                            </div>
                        </div>

                        <div id="category">
                            <div class="title" data-i18n="anlihouse-A268"></div>

                            <span class="plus" style="display:none">
                                <span class="badge elem single filterCategoryAll active" data-name="0"
                                      data-status="active">
                                    <span class="text" data-i18n="anlihouse-A269"></span>
                                </span>

                                <span class="badge elem plus-icon">
                                    <svg width="16" height="16" viewBox="0 0 752 752"
                                         xmlns="http://www.w3.org/2000/svg"><defs><clipPath class="p"><path
                                            d="m139.21 139.21h473.58v473.58h-473.58z"/></clipPath></defs><g
                                            clip-path="url(.p)"><path
                                            d="m318.6 139.21h114.8v179.39h179.39v114.8h-179.39v179.39h-114.8v-179.39h-179.39v-114.8h179.39z"
                                            fill-rule="evenodd"/></g>
                                    </svg>
                                </span>
                            </span>

                            <div class="badge-group active">
                                <span class="badge elem single filterCategoryAll active" data-name="0"
                                      data-status="active">
                                    <span class="text" data-i18n="anlihouse-A269"></span>
                                </span>

                                {% for category in filter_category_all %}
                                    {% if category['lang'] == "hu" %}
                                        <span class="badge elem multiple inactive" data-name="{{ category['id'] }}"
                                              data-status="inactive">
                                            <span class="text">{{ category['name_hu'] }}</span>
                                        </span>
                                    {% elif category['lang'] == "en" %}
                                        <span class="badge elem multiple inactive" data-name="{{ category['id'] }}"
                                              data-status="inactive">
                                            <span class="text">{{ category['name_en'] }}</span>
                                        </span>
                                    {% elif category['lang'] == "de" %}
                                        <span class="badge elem multiple inactive" data-name="{{ category['id'] }}"
                                              data-status="inactive">
                                            <span class="text">{{ category['name_de'] }}</span>
                                        </span>
                                    {% elif category['lang'] == "fr" %}
                                        <span class="badge elem multiple inactive" data-name="{{ category['id'] }}"
                                              data-status="inactive">
                                            <span class="text">{{ category['name_fr'] }}</span>
                                        </span>
                                    {% elif category['lang'] == "es" %}
                                        <span class="badge elem multiple inactive" data-name="{{ category['id'] }}"
                                              data-status="inactive">
                                            <span class="text">{{ category['name_es'] }}</span>
                                        </span>
                                    {% endif %}
                                {% endfor %}

