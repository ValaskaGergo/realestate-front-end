{# Start Breadcrumb #}
{% block breadcrumb %}
    <div id="breadcrumb">

        <div class="container w-100 mw-100 pl-3 pr-3">
            <div class="row">
                <div class="col-12">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a class="hover" href="/" data-i18n="anlihouse-anlihouse"></a>
                            </li>
                            <li class="breadcrumb-item"><a class="hover" href="/"
                                                           data-i18n="anlihouse-anlibreeders"></a></li>
                            <li class="breadcrumb-item"><a class="hover"
                                                           href="{{ url_for('index_module.category_list', category_name=animal['animal']['category']['name_en_slug'], category_id=animal['animal']['category']['id']) }}">{{ category }}</a>
                            </li>
                            <li class="breadcrumb-item"><a class="hover"
